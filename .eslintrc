{
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 10
    },
    "globals": {},
    "extends": ["airbnb-base", "prettier"],
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "Sequelize"
            }
        ]
    }
}
